{
  "name": "main-system",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "umi dev",
    "build": "cross-env BUILD_TYPE=test umi build",
    "build:pro": "cross-env BUILD_TYPE=pro umi build",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "gene": "node swagger.generate.js",
    "eslint": "umi lint --eslint-only --quiet",
    "eslint:fix": "umi lint --eslint-only --fix",
    "stylelint": "umi lint --stylelint-only --quiet",
    "ts-check": "tsc -p tsconfig.check.json",
    "check-all": "npm run ts-check && npm run eslint && npm run stylelint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-layout": "^7.19.0",
    "antd": "^5.17.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "qs": "^6.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "umi": "^4.2.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.208",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@umijs/lint": "^4.2.1",
    "@umijs/plugins": "^4.2.1",
    "@unocss/cli": "^0.60.0",
    "@unocss/preset-icons": "^0.60.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "stylelint": "^16.5.0",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.4.5",
    "unocss": "^0.60.0",
    "webpack-chain": "^6.5.1"
  }
}
