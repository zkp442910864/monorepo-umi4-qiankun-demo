    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "umi": "^4.0.79"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.103",
    "@types/duplicate-package-checker-webpack-plugin": "^2.1.2",
